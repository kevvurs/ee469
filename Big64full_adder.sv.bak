module Big64full_adder(a, b, cout, sum);
	input logic [63:0] a, b;
	input logic [63:0] cout, sum;
	
	full_adder halfAdder(.a(a[i]), .b(b[i]), .cin(0'b0), .sum(sum[0]), .cout(cout[0]));
	
	genvar i;
	generate
		for (i=1; i<64; i++) begin
			full_adder fullAdder(.a(a[i]), .b(b[i]), .cin(cout[i - 1]), .cout(cout[i]), .sum(sum[i]));
		end
	endgenerate
endmodule
